<?xml version="1.0" encoding="UTF-8"?><anychart>
      <settings>
        <locale>
          <date_time_format>
            <format/>
          </date_time_format>
        </locale>
        <animation enabled="true"/></settings>

  <charts>
    <chart plot_type="doughnut">
    
    
    
      <styles><bar_style name="previous" parent="parent">
	<fill color="blue" opacity="0.2"/>
	<states>
		<hover>
			<fill color="yellow"/></hover></states></bar_style>
      	<bar_style name="parent">
      		<fill color="blue" opacity="1" type="solid"/>
      		<states>
      			<hover>
      				<fill color="yellow"/></hover></states></bar_style></styles><data_plot_settings enable_3d_mode="false">
        <pie_series>
          <tooltip_settings enabled="true">
<format>{%Value}{scale:(1)(1000)(1000)|(W)(KW)(MW),numDecimals:2}</format>
          	<position anchor="CenterTop" padding="10"/></tooltip_settings>
          
          <label_settings enabled="true">
          	<position anchor="Center" halign="Center" padding="0" valign="Center">
          	</position>
          	<background enabled="false"/>
          	<font color="White" size="11">
          		<effects>
          			<drop_shadow blur_x="2" blur_y="2" distance="1" enabled="true" opacity="0.9">
          			</drop_shadow></effects></font>
          	<format>{%YPercentOfSeries}{numDecimals:1}%</format></label_settings><marker_settings enabled="true">
          	<marker type="None"/>
          	<states>
          		<hover>
          			<marker anchor="CenterTop" type="Circle"/></hover></states></marker_settings>
          </pie_series></data_plot_settings>

      <chart_settings>
    <chart_background>
        	<border color="blue" enabled="false"/>
        	<inside_margin all="0"/>
        	<fill color="red" enabled="false" type="solid"/></chart_background><data_plot_background>
		<border enabled="false"/><fill color="white" enabled="true" opacity="0.5" type="solid"/>
		</data_plot_background>



      	<title enabled="true" padding="20">
      		<text/>
      		<font color="Blue">
      			<effects enabled="true">
      				<drop_shadow distance="1" enabled="False" opacity="1"/>
      			</effects>
      		</font>
      	</title>
      	<axes>
		  <y_axis>
		  	<title><text/><font color="#003399"/></title>
			<scale type="Linear"/>
            <labels>
				<format/>
			

						<font color="Gray">
							<effects>
								<drop_shadow distance="1" enabled="False" opacity="1"/>
							</effects>
						</font>
</labels>
            
            <line color="Gray" opacity="1"/>
            <major_tickmark color="Gray"/>
            <minor_tickmark color="Gray"/>
            <minor_grid enabled="false"/>
           <major_grid interlaced="True">
						<line color="Gray" opacity="0.7"/>
					</major_grid></y_axis>		  
          <extra>
          	<y_axis enabled="False" name="y2">
          		 <major_tickmark color="White"/>
            <minor_tickmark color="White"/>
          		 <line color="Gray" opacity="1"/>
          		<title rotation="180">
          			<text>m3/hr Agua</text>
          			
          			<font color="#ff0099"/></title>
          			
          	<labels>
          				<format/>
          				
          			
						<font color="Gray">
							<effects>
								<drop_shadow distance="1" enabled="false" opacity="1"/>
							</effects>
						</font>
          				
          				
          				</labels>		
          			
          		<scale maximum="1" minimum="1"/></y_axis>
          	<y_axis enabled="False" name="y3">
          	<major_tickmark color="Gray"/>
            <minor_tickmark color="Gray"/>
          		 <line color="Black" opacity="1"/>
          		<labels>
          				<format/>
          				
          			
						<font color="Gray">
							<effects>
								<drop_shadow distance="1" enabled="false" opacity="1"/>
							</effects>
						</font>
          				
          				
          				</labels>
          		<title rotation="180">
          			<text>m3/hr Gas</text>
          			<font color="#00cccc"/>
          		</title>
          		<scale maximum="1" minimum="1"/></y_axis></extra><x_axis>
			<scale type="DateTime"/>
			<major_tickmark color="Gray"/>
					<minor_tickmark color="Gray"/>
            <labels>
				<format/>
				<font color="Gray">
							<effects enabled="False">
								<drop_shadow distance="1" enabled="true" opacity="1"/>
							</effects>
						</font>
            </labels>
            <title enabled="true">
              <text/>
            </title>
            <minor_grid enabled="false">
            	</minor_grid><major_grid enabled="true">
            	<line color="Gray" opacity="0.4"/></major_grid>
	<line color="gray" enabled="true" opacity="0.5"/></x_axis>
          </axes>
      	<legend enabled="true" ignore_auto_item="true" position="right">
      		<title enabled="false"/>
      		<items>
      			<item source="points"/></items></legend></chart_settings>
      <data>
      <series name="Series 1" type="Pie"><point name="0-6 AM" y="102.70889964"/><point name="6-12 PM" y="453.06733626"/><point name="12-18 AM" y="531.78271686"/><point name="18-24 AM" y="193.6902834"/></series></data>
      </chart>
  </charts>
</anychart>